{
    "name": "vendocrat/laravel-meta",
    "description": "Simple and fluent meta data for Eloquent Models in Laravel 5.",
    "homepage" : "https://github.com/AlexanderPoellmann/laravel-meta",
    "license": "MIT",
    "keywords": [
        "meta",
        "meta data",
        "metable",
        "laravel"
    ],
    "authors": [
        {
            "name"     : "Alexander Manfred Poellmann",
            "homepage" : "http://twitter.com/AMPoellmann",
            "role"     : "developer"
        }
    ],
    "require": {
        "php"                   : ">=5.5.9",
        "doctrine/dbal"         : "2.5.x",
        "illuminate/support"    : ">=5.1",
        "illuminate/database"   : ">=5.1"
    },
    "require-dev": {
        "mockery/mockery" : "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "vendocrat\\Meta\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/AlexanderPoellmann/laravel-meta",
        "issues" : "https://github.com/AlexanderPoellmann/laravel-meta/issues"
    }
}
