{
    "name": "lecturize/laravel-meta",
    "description": "Simple and fluent meta data for Eloquent Models in Laravel 5.",
    "homepage" : "https://github.com/Lecturize/Laravel-Meta",
    "license": "MIT",
    "keywords": [
        "meta",
        "meta data",
        "metable",
        "eloquent",
        "model",
        "laravel"
    ],
    "authors": [
        {
            "name"     : "Alexander Manfred Poellmann",
            "homepage" : "http://twitter.com/AMPoellmann",
            "role"     : "developer"
        }
    ],
    "require": {
        "php"                 : ">=5.6.4|^7.0",
        "illuminate/support"  : ">=5.3",
        "illuminate/database" : ">=5.3"
    },
    "require-dev": {
        "laravel/framework" : "5.3.*",
        "mockery/mockery" : "0.9.*",
        "phpunit/phpunit" : "^6.0",
        "doctrine/dbal"   : "2.5.x"
    },
    "autoload": {
        "psr-4": {
            "Lecturize\\Meta\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/Lecturize/Laravel-Meta",
        "issues" : "https://github.com/Lecturize/Laravel-Meta/issues"
    }
}
